#include <iostream>

int main() {
    // Loop 'for' que começa em 0, continua enquanto 'i' for menor ou igual a 10,
    // e incrementa 'i' em 1 a cada iteração.
    for (int i = 0; i <= 10; ++i) {
        std::cout << i << std::endl; // Imprime o valor atual de 'i' e pula para a próxima linha
    }

    return 0; // Indica que o programa terminou com sucesso
}